  font-size: 24rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
  line-height: 33rpx;
  float: left;
  margin-left: 10rpx;
}

.articleOne>image {
  width: 216rpx;
  height: 170rpx;
  margin: 24rpx 0 24rpx 54rpx;
  display: inline-block;
}

.articleTwo {
  width: 686rpx;
  height: 136rpx;
  margin-left: 32rpx;
  border-bottom: 1rpx solid #e5e5e5;
}

.articleTwo text {
  width: 686rpx;
  height: 45rpx;
  font-size: 32rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 45rpx;
  margin-top: 24rpx;
  margin-bottom: 10rpx;
}

