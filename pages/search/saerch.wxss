/* pages/search/saerch.wxss */
page {
  height: 100%;
  background: #fff;
}

.container {
  box-sizing: border-box;
  position: relative;
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 2;
  box-shadow: 0px 1px 4px 0px rgba(0, 0, 0, 0.1);
}

.title-bar {
  display: flex;
  align-items: center;
}

.title-bar>view {
  height: 60rpx;
  width: 460rpx;
  /* background: #fff; */
  border-radius: 30rpx;
  float: left;
  line-height: 60rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.back {
  width: 18rpx;
  height: 30rpx;
  display: inline-block;
  padding: 20rpx;
  margin-left: 12rpx;
  margin-right: 4rpx;
}

.evahospital {
  width: 610rpx;
  text-align: center;
  opacity: 1;
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  letter-spacing: 0rpx;
}

.searchBox {
  width: 640rpx;
  height: 60rpx;
  background: rgba(0, 0, 0, 0.05);
  border-radius: 30rpx;
  line-height: 60rpx;
  font-size: 30rpx;
}
.searchBox image{
  width: 26rpx;
  height: 29rpx;
  margin-left: 20rpx;
  margin-top: 16rpx;
  float: left;
}
.searchBox>input{
  width: 273rpx;
  height: 60rpx;
  display: inline-block;
  float: left;
  margin-left: 12rpx;
  padding-right: 10rpx;
}
.searchBox>view{
  display: inline-block;
  width: 120rpx;
height: 60rpx;
background: #ed2828;
color: #fff;
border-radius: 0rpx 30rpx 30rpx 0rpx;
text-align: center;
float: left;
}