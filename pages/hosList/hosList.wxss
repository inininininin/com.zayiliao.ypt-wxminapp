/* pages/hosList/hosList.wxss */
@import '../../app.wxss';
page {
  height: 100%;
  background: #fff;
}

.container {
  box-sizing: border-box;
  position: relative;
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 2;
}

.title-bar {
  display: flex;
  align-items: center;
}

.title-bar>view {
  height: 60rpx;
  width: 460rpx;
  background: #fff;
  border-radius: 30rpx;
  float: left;
  line-height: 60rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.back {
  width: 18rpx;
  height: 30rpx;
  display: inline-block;
  padding: 20rpx;
  margin-left: 12rpx;
}
.evahospital {
  width: 610rpx;
  text-align: center;
  opacity: 1;
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  letter-spacing: 0rpx;
  /* margin: 0 auto; */
}
.main{
  width: 100%;
  height:auto;
  border-top: 1rpx solid #e5e5e5;
}
.searchBox{
  width: 100%;
  height: 100rpx;
}
.searchBox input{
  width: 650rpx;
  height: 70rpx;
  border-radius: 35rpx;
  padding: 0 20rpx;
  font-size: 32rpx;
  line-height: 70rpx;
  margin: 10rpx 30rpx;
  border: 1rpx solid #e5e5e5;
  display: inline-block;
}
.searchList{
  width: 690rpx;
  height: auto;
  margin: 10rpx 30rpx;
}
.searchList>text{
  display: block;
  height: 75rpx;
  width: 100%;
  line-height: 75rpx;
  font-size: 30rpx;
  text-align: left;
  border-bottom: 0.5px solid #e5e5e5;
}
