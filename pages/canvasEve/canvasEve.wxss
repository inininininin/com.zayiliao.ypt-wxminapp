/* pages/canvasHos/canvasHos.wxss */
page {
  height: 100%;
  background: #fff;
}

.container {
  box-sizing: border-box;
  position: relative;
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 2;
  box-shadow: 0px 1px 4px 0px rgba(0,0,0,0.1);
}

.title-bar {
  display: flex;
  align-items: center;
}

.title-bar>view {
  height: 60rpx;
  width: 460rpx;
  background: #fff;
  border-radius: 30rpx;
  float: left;
  line-height: 60rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

.back {
  width: 18rpx;
  height: 30rpx;
  display: inline-block;
  padding: 20rpx;
  margin-left: 12rpx;
}

.evahospital {
  width: 610rpx;
  text-align: center;
  opacity: 1;
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  letter-spacing: 0rpx;
}
